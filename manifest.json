{
  "name": "CF-GitGud",
  "description": "Utils for Codeforces",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://*.codeforces.com/problemset/problem/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/logo.png"
  }
}
